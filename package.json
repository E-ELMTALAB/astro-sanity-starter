{
  "name": "astro-sanity-starter",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "create-project": "node ./sanity-export/create-project.js",
    "import": "node ./sanity-export/import.js",
    "export": "node ./sanity-export/export.js",
    "build:astro": "pnpm run build",
    "build:next": "pnpm --filter telegram-shop run build",
    "test": "pnpm check:all",
    "test:static": "node --loader ts-node/esm tests/run.mts sanity && node --loader ts-node/esm tests/run.mts stackbit && node --loader ts-node/esm tests/run.mts typescript && node --loader ts-node/esm tests/run.mts components && node --loader ts-node/esm tests/run.mts homepage && node --loader ts-node/esm tests/run.mts react && node --loader ts-node/esm tests/run.mts content && node --loader ts-node/esm tests/run.mts safety && node --loader ts-node/esm tests/run.mts legacy",
    "test:render": "node --loader ts-node/esm tests/run.mts render",
    "test:sanity": "node --loader ts-node/esm tests/run.mts sanity",
    "test:stackbit": "node --loader ts-node/esm tests/run.mts stackbit",
    "test:typescript": "node --loader ts-node/esm tests/run.mts typescript",
    "test:components": "node --loader ts-node/esm tests/run.mts components",
    "test:homepage": "node --loader ts-node/esm tests/run.mts homepage",
    "test:react": "node --loader ts-node/esm tests/run.mts react",
    "test:content": "node --loader ts-node/esm tests/run.mts content",
    "test:safety": "node --loader ts-node/esm tests/run.mts safety",
    "test:legacy": "node --loader ts-node/esm tests/run.mts legacy",
    "test:section": "node --loader ts-node/esm tests/run.mts sanity && node --loader ts-node/esm tests/run.mts stackbit",
    "check:static": "pnpm test:static",
    "check:render": "pnpm test:render",
    "check:all": "node --loader ts-node/esm tests/run.mts all",
    "check:section": "pnpm test:section",
    "ci:astro": "pnpm check:static && pnpm build:astro && pnpm check:render",
    "ci:full": "pnpm check:static && pnpm build:astro && pnpm check:render && pnpm build:next",
    "ci:test": "pnpm ci:astro",
    "pre-commit": "pnpm check:static",
    "pre-push": "pnpm ci:astro",
    "validate": "pnpm check:all"
  },
  "dependencies": {
    "@astrojs/react": "^4.0.7",
    "@sanity/astro": "^3.1.6",
    "@sanity/client": "^6.22.0",
    "@sanity/validation": "^3.14.4",
    "@tailwindcss/vite": "^4.0.9",
    "astro": "^5.1.1",
    "astro-icon": "^1.1.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.471.2",
    "marked": "^15.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.0.9",
    "vite": "^7.1.7"
  },
  "devDependencies": {
    "@sanity/export": "^3.38.1",
    "@sanity/import": "^3.37.4",
    "@stackbit/cms-sanity": "^0.2.86",
    "@stackbit/types": "^2.0.0",
    "configstore": "7.0.0",
    "daisyui": "^5.0.0",
    "fast-glob": "^3.3.2",
    "groq-js": "^1.10.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "ts-node": "^10.9.2",
    "vitest": "^2.1.1",
    "zod": "^3.23.8"
  }
}